{
    "name": "xannn94/modules",
    "description": "Laravel 5 Modules",
    "keywords": ["modules", "laravel", "xannn94"],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Alex Khan",
            "email": "axiles94@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*",
        "illuminate/support": "5.3.*",
        "illuminate/filesystem": "5.3.*",
        "illuminate/config": "5.3.*",
        "illuminate/routing": "5.3.*"
